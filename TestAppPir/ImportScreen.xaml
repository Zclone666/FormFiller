<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TestAppPir.ImportScreen"
             Title="ImportScreen">
    <VerticalStackLayout>
        <VerticalStackLayout.Background>
            <LinearGradientBrush EndPoint="1,0">
                <GradientStop Color="#AEC09A"
                          Offset="0.1" />
                <GradientStop Color="#AEC670"
                          Offset="1.0" />
            </LinearGradientBrush>
        </VerticalStackLayout.Background>
        <Button Text="Import" HorizontalOptions="Center" x:Name="Import" Clicked="Import_Clicked" TextColor="#fffff0" BackgroundColor="#344c11"/>
    </VerticalStackLayout>
</ContentPage>